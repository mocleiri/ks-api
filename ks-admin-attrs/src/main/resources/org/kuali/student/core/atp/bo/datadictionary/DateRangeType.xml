<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
>

    <bean id="DateRangeType" parent="DateRangeType-parentBean" />

    <bean id="DateRangeType-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.student.core.atp.bo.DateRangeType" />
        <property name="titleAttribute" value="id" />
        <property name="objectLabel" value="Date Range Type" />
        <property name="lookupDefinition">
            <ref bean="DateRangeType-lookupDefinition" />
        </property>
        <property name="inquiryDefinition">
            <ref bean="DateRangeType-inquiryDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="DateRangeType-id" />
                <ref bean="DateRangeType-name" />
                <ref bean="DateRangeType-description" />
                <ref bean="DateRangeType-activeFromDate" />
                <ref bean="DateRangeType-activeToDate" />
                <ref bean="DateRangeType-active" />
            </list>
        </property>
    </bean>

    <bean id="DateRangeType-id" parent="DateRangeType-id-parentBean" />

    <bean id="DateRangeType-id-parentBean" abstract="true" parent="AttributeReferenceKualiType-id">
    </bean>

    <bean id="DateRangeType-name" parent="DateRangeType-name-parentBean" />

    <bean id="DateRangeType-name-parentBean" abstract="true" parent="AttributeReferenceKualiType-name">
    </bean>

    <bean id="DateRangeType-description" parent="DateRangeType-description-parentBean" />

    <bean id="DateRangeType-description-parentBean" abstract="true" parent="AttributeReferenceKualiType-description">
    </bean>

    <bean id="DateRangeType-activeFromDate" parent="DateRangeType-activeFromDate-parentBean" />

    <bean id="DateRangeType-activeFromDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeFromDate">
    </bean>

    <bean id="DateRangeType-activeToDate" parent="DateRangeType-activeToDate-parentBean" />

    <bean id="DateRangeType-activeToDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeToDate">
    </bean>

    <bean id="DateRangeType-active" parent="DateRangeType-active-parentBean" />

    <bean id="DateRangeType-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
        <property name="name" value="active" />
    </bean>


    <bean id="DateRangeType-lookupDefinition" parent="DateRangeType-lookupDefinition-parentBean" />

    <bean id="DateRangeType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="name" />
                <bean parent="FieldDefinition" p:attributeName="active" />
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="name" />
                <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
                <bean parent="FieldDefinition" p:attributeName="activeToDate" />
            </list>
        </property>
        <property name="title" value="Date Range Type Lookup" />
    </bean>

    <bean id="DateRangeType-inquiryDefinition" parent="DateRangeType-inquiryDefinition-parentBean" />

    <bean id="DateRangeType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Date Range Type Inquiry" />
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Date Range Type" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id" />
                            <bean parent="FieldDefinition" p:attributeName="name" />
                            <bean parent="FieldDefinition" p:attributeName="description" />
                            <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
                            <bean parent="FieldDefinition" p:attributeName="activeToDate" />
                        </list>
                    </property>
                </bean>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Extension Attributes" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="InquiryCollectionDefinition">
                                <property name="attributeName" value="attributes" />
                                <property name="businessObjectClass" value="org.kuali.rice.kns.bo.BusinessObjectExtensionAttributeImpl" />
                                <property name="numberOfColumns" value="1" />
                                <property name="summaryTitle" value="Attribute" />
                                <property name="inquiryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="name" />
                                        <bean parent="FieldDefinition" p:attributeName="value" />
                                    </list>
                                </property>
                                <property name="summaryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="name" />
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>                
            </list>
        </property>
    </bean>

</beans>