<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
>

    <bean id="AtpSeasonalType" parent="AtpSeasonalType-parentBean" />

    <bean id="AtpSeasonalType-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.student.core.atp.bo.AtpSeasonalType" />
        <property name="titleAttribute" value="id" />
        <property name="objectLabel" value="Academic Time Period Seasonal Type" />
        <property name="lookupDefinition">
            <ref bean="AtpSeasonalType-lookupDefinition" />
        </property>
        <property name="inquiryDefinition">
            <ref bean="AtpSeasonalType-inquiryDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="AtpSeasonalType-id" />
                <ref bean="AtpSeasonalType-name" />
                <ref bean="AtpSeasonalType-description" />
                <ref bean="AtpSeasonalType-activeFromDate" />
                <ref bean="AtpSeasonalType-activeToDate" />
                <ref bean="AtpSeasonalType-active" />
            </list>
        </property>
    </bean>

    <bean id="AtpSeasonalType-id" parent="AtpSeasonalType-id-parentBean" />

    <bean id="AtpSeasonalType-id-parentBean" abstract="true" parent="AttributeReferenceKualiType-id">
    </bean>

    <bean id="AtpSeasonalType-name" parent="AtpSeasonalType-name-parentBean" />

    <bean id="AtpSeasonalType-name-parentBean" abstract="true" parent="AttributeReferenceKualiType-name">
    </bean>

    <bean id="AtpSeasonalType-description" parent="AtpSeasonalType-description-parentBean" />

    <bean id="AtpSeasonalType-description-parentBean" abstract="true" parent="AttributeReferenceKualiType-description">
    </bean>

    <bean id="AtpSeasonalType-activeFromDate" parent="AtpSeasonalType-activeFromDate-parentBean" />

    <bean id="AtpSeasonalType-activeFromDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeFromDate">
    </bean>

    <bean id="AtpSeasonalType-activeToDate" parent="AtpSeasonalType-activeToDate-parentBean" />

    <bean id="AtpSeasonalType-activeToDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeToDate">
    </bean>

    <bean id="AtpSeasonalType-active" parent="AtpSeasonalType-active-parentBean" />

    <bean id="AtpSeasonalType-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
        <property name="name" value="active" />
    </bean>


    <bean id="AtpSeasonalType-lookupDefinition" parent="AtpSeasonalType-lookupDefinition-parentBean" />

    <bean id="AtpSeasonalType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="name" />
                <bean parent="FieldDefinition" p:attributeName="active" />
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="name" />
                <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
                <bean parent="FieldDefinition" p:attributeName="activeToDate" />
            </list>
        </property>
        <property name="title" value="Atp Seasonal Type Lookup" />
    </bean>

    <bean id="AtpSeasonalType-inquiryDefinition" parent="AtpSeasonalType-inquiryDefinition-parentBean" />

    <bean id="AtpSeasonalType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Atp Seasonal Type Inquiry" />
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Seasonal Type" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id" />
                            <bean parent="FieldDefinition" p:attributeName="name" />
                            <bean parent="FieldDefinition" p:attributeName="description" />
                            <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
                            <bean parent="FieldDefinition" p:attributeName="activeToDate" />
                        </list>
                    </property>
                </bean>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Extension Attributes" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="InquiryCollectionDefinition">
                                <property name="attributeName" value="attributes" />
                                <property name="businessObjectClass" value="org.kuali.rice.kns.bo.BusinessObjectExtensionAttributeImpl" />
                                <property name="numberOfColumns" value="1" />
                                <property name="summaryTitle" value="Attribute" />
                                <property name="inquiryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="name" />
                                        <bean parent="FieldDefinition" p:attributeName="value" />
                                    </list>
                                </property>
                                <property name="summaryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="name" />
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>                
            </list>
        </property>
    </bean>

</beans>