<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
>

    <bean id="MilestoneType" parent="MilestoneType-parentBean" />

    <bean id="MilestoneType-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.student.core.atp.bo.MilestoneType" />
        <property name="titleAttribute" value="id" />
        <property name="objectLabel" value="Milestone Type" />
        <property name="lookupDefinition">
            <ref bean="MilestoneType-lookupDefinition" />
        </property>
        <property name="inquiryDefinition">
            <ref bean="MilestoneType-inquiryDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="MilestoneType-id" />
                <ref bean="MilestoneType-name" />
                <ref bean="MilestoneType-description" />
                <ref bean="MilestoneType-activeFromDate" />
                <ref bean="MilestoneType-activeToDate" />
                <ref bean="MilestoneType-active" />
            </list>
        </property>
    </bean>

    <bean id="MilestoneType-id" parent="MilestoneType-id-parentBean" />

    <bean id="MilestoneType-id-parentBean" abstract="true" parent="AttributeReferenceKualiType-id">
    </bean>

    <bean id="MilestoneType-name" parent="MilestoneType-name-parentBean" />

    <bean id="MilestoneType-name-parentBean" abstract="true" parent="AttributeReferenceKualiType-name">
    </bean>

    <bean id="MilestoneType-description" parent="MilestoneType-description-parentBean" />

    <bean id="MilestoneType-description-parentBean" abstract="true" parent="AttributeReferenceKualiType-description">
    </bean>

    <bean id="MilestoneType-activeFromDate" parent="MilestoneType-activeFromDate-parentBean" />

    <bean id="MilestoneType-activeFromDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeFromDate">
    </bean>

    <bean id="MilestoneType-activeToDate" parent="MilestoneType-activeToDate-parentBean" />

    <bean id="MilestoneType-activeToDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeToDate">
    </bean>

    <bean id="MilestoneType-active" parent="MilestoneType-active-parentBean" />

    <bean id="MilestoneType-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
        <property name="name" value="active" />
    </bean>


    <bean id="MilestoneType-lookupDefinition" parent="MilestoneType-lookupDefinition-parentBean" />

    <bean id="MilestoneType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="name" />
                <bean parent="FieldDefinition" p:attributeName="active" />
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="name" />
                <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
                <bean parent="FieldDefinition" p:attributeName="activeToDate" />
            </list>
        </property>
        <property name="title" value="Milestone Type Lookup" />
    </bean>

    <bean id="MilestoneType-inquiryDefinition" parent="MilestoneType-inquiryDefinition-parentBean" />

    <bean id="MilestoneType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Milestone Type Inquiry" />
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Milestone Type" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id" />
                            <bean parent="FieldDefinition" p:attributeName="name" />
                            <bean parent="FieldDefinition" p:attributeName="description" />
                            <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
                            <bean parent="FieldDefinition" p:attributeName="activeToDate" />
                        </list>
                    </property>
                </bean>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Extension Attributes" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="InquiryCollectionDefinition">
                                <property name="attributeName" value="attributes" />
                                <property name="businessObjectClass" value="org.kuali.rice.kns.bo.BusinessObjectExtensionAttributeImpl" />
                                <property name="numberOfColumns" value="1" />
                                <property name="summaryTitle" value="Attribute" />
                                <property name="inquiryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="name" />
                                        <bean parent="FieldDefinition" p:attributeName="value" />
                                    </list>
                                </property>
                                <property name="summaryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="name" />
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>                
            </list>
        </property>
    </bean>

</beans>