<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
>

    <bean id="AtpDurationType" parent="AtpDurationType-parentBean" />

    <bean id="AtpDurationType-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.student.core.atp.bo.AtpDurationType" />
        <property name="titleAttribute" value="id" />
        <property name="objectLabel" value="Academic Time Period Duration Type" />
        <property name="lookupDefinition">
            <ref bean="AtpDurationType-lookupDefinition" />
        </property>
        <property name="inquiryDefinition">
            <ref bean="AtpDurationType-inquiryDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="AtpDurationType-id" />
                <ref bean="AtpDurationType-name" />
                <ref bean="AtpDurationType-description" />
                <ref bean="AtpDurationType-activeFromDate" />
                <ref bean="AtpDurationType-activeToDate" />
                <ref bean="AtpDurationType-active" />
            </list>
        </property>
    </bean>

    <bean id="AtpDurationType-id" parent="AtpDurationType-id-parentBean" />

    <bean id="AtpDurationType-id-parentBean" abstract="true" parent="AttributeReferenceKualiType-id">
    </bean>

    <bean id="AtpDurationType-name" parent="AtpDurationType-name-parentBean" />

    <bean id="AtpDurationType-name-parentBean" abstract="true" parent="AttributeReferenceKualiType-name">
    </bean>

    <bean id="AtpDurationType-description" parent="AtpDurationType-description-parentBean" />

    <bean id="AtpDurationType-description-parentBean" abstract="true" parent="AttributeReferenceKualiType-description">
    </bean>

    <bean id="AtpDurationType-activeFromDate" parent="AtpDurationType-activeFromDate-parentBean" />

    <bean id="AtpDurationType-activeFromDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeFromDate">
    </bean>

    <bean id="AtpDurationType-activeToDate" parent="AtpDurationType-activeToDate-parentBean" />

    <bean id="AtpDurationType-activeToDate-parentBean" abstract="true" parent="AttributeReferenceDummy-activeToDate">
    </bean>

    <bean id="AtpDurationType-active" parent="AtpDurationType-active-parentBean" />

    <bean id="AtpDurationType-active-parentBean" abstract="true" parent="AttributeReferenceDummy-activeIndicator">
        <property name="name" value="active" />
    </bean>


    <bean id="AtpDurationType-lookupDefinition" parent="AtpDurationType-lookupDefinition-parentBean" />

    <bean id="AtpDurationType-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="name" />
                <bean parent="FieldDefinition" p:attributeName="active" />
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="name" />
                <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
                <bean parent="FieldDefinition" p:attributeName="activeToDate" />
            </list>
        </property>
        <property name="title" value="Atp Duration Type Lookup" />
    </bean>

    <bean id="AtpDurationType-inquiryDefinition" parent="AtpDurationType-inquiryDefinition-parentBean" />

    <bean id="AtpDurationType-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="title" value="Atp Duration Type Inquiry" />
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Duration Type" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id" />
                            <bean parent="FieldDefinition" p:attributeName="name" />
                            <bean parent="FieldDefinition" p:attributeName="description" />
                            <bean parent="FieldDefinition" p:attributeName="activeFromDate" />
                            <bean parent="FieldDefinition" p:attributeName="activeToDate" />
                        </list>
                    </property>
                </bean>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Extension Attributes" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="InquiryCollectionDefinition">
                                <property name="attributeName" value="attributes" />
                                <property name="businessObjectClass" value="org.kuali.rice.kns.bo.BusinessObjectExtensionAttributeImpl" />
                                <property name="numberOfColumns" value="1" />
                                <property name="summaryTitle" value="Attribute" />
                                <property name="inquiryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="name" />
                                        <bean parent="FieldDefinition" p:attributeName="value" />
                                    </list>
                                </property>
                                <property name="summaryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="name" />
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>                
            </list>
        </property>
    </bean>

</beans>