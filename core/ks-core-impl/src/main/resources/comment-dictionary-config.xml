	<dict:fieldDescriptor id="baseString" parent="baseString-parent"/>
	<dict:fieldDescriptor id="baseString-parent"  abstract="true">
		<dict:dataType>string</dict:dataType>
		<dict:readOnly>false</dict:readOnly>
	</dict:fieldDescriptor>

