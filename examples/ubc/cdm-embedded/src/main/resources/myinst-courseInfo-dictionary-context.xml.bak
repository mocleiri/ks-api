<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://student.kuali.org/xsd/dictionary-extension http://student.kuali.org/xsd/dictionary-extension/dictionary-extension.xsd">
 <import resource="classpath:ks-courseInfo-dictionary-context.xml" />


 <!-- CourseInfo-->
 <bean id="org.kuali.student.lum.course.dto.CourseInfo-parent" parent="objectStructureDefinition">
  <property name="attributes">
   <list merge=true>
     <ref bean="courseInfo.changeCategory" />
   </list>
  </property>
 </bean>

 <bean id="courseInfo.changeCategory" parent="baseString">
  <property name="name" value="changeCategory" />
  <property name="dynamic" value="true"/>
  <property name="minOccurs" value="0" />
  <property name="maxOccurs" value="1" />
  <property name="maxLength" value="1" />
  <property name="validChars" ref="digits"/>
 </bean>

</beans>