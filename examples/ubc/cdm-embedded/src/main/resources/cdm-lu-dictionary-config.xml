<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:dict="http://student.kuali.org/xsd/dictionary-extension"
xsi:schemaLocation="
http://student.kuali.org/xsd/dictionary-extension http://student.kuali.org/xsd/dictionary-extension/dictionary-extension.xsd
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
">


	<dict:constraintDescriptor key="shortName" id="dict.course.official.transcriptTitle.cd" parent="dict.course.official.transcriptTitle.cd.abstract">
		<dict:constraint key="in-line.constraint.for.dictionary.course.official.transcriptTitle">
			<dict:maxLength>10</dict:maxLength>
		</dict:constraint>	
	</dict:constraintDescriptor>
	
	<dict:constraintDescriptor key="shortName" id="dict.course.official.transcriptTitle.draft.cd" parent="dict.course.official.transcriptTitle.draft.cd.abstract">	
		<dict:constraint key="in-line.constraint.for.dictionary.course.official.transcriptTitle.draft">
			<dict:maxLength>10</dict:maxLength>
		</dict:constraint>
	</dict:constraintDescriptor>
	<dict:constraintDescriptor key="shortName" id="dict.course.version.transcriptTitle.cd" parent="dict.course.version.transcriptTitle.cd.abstract">
		<dict:constraint key="in-line.constraint.for.dictionary.course.version.transcriptTitle">
			<dict:maxLength>10</dict:maxLength>
		</dict:constraint>	
	</dict:constraintDescriptor>

	<dict:constraintDescriptor key="shortName" id="dict.course.version.transcriptTitle.draft.cd" parent="dict.course.version.transcriptTitle.draft.cd.abstract">
		<dict:constraint key="in-line.constraint.for.dictionary.course.version.transcriptTitle.draft">
			<dict:maxLength>10</dict:maxLength>
		</dict:constraint>	
	</dict:constraintDescriptor>
	

	
	<dict:constraintDescriptor key="longName" id="dict.course.official.catalogTitle.cd" parent="dict.course.official.catalogTitle.cd.abstract">
		<dict:constraint key="in-line.constraint.for.dictionary.course.official.catalogTitle">
			<dict:maxLength>10</dict:maxLength>
		</dict:constraint>	
	</dict:constraintDescriptor>
	
	<dict:constraintDescriptor key="longName" id="dict.course.official.catalogTitle.draft.cd" parent="dict.course.official.catalogTitle.draft.cd.abstract">
		<dict:constraint key="in-line.constraint.for.dictionary.course.official.catalogTitle.draft">
			<dict:maxLength>10</dict:maxLength>
		</dict:constraint>	
	</dict:constraintDescriptor>
	
	<dict:constraintDescriptor key="longName" id="dict.course.version.catalogTitle.cd" parent="dict.course.version.catalogTitle.cd.abstract">
		<dict:constraint key="in-line.constraint.for.dictionary.course.version.catalogTitle">
			<dict:maxLength>10</dict:maxLength>
		</dict:constraint>	
	</dict:constraintDescriptor>
	
	<dict:constraintDescriptor key="longName" id="dict.course.format.official.name.cd" parent="dict.course.format.official.name.cd.abstract">
		<dict:constraint key="in-line.constraint.for.dictionary.course.format.official.name">
			<dict:maxLength>10</dict:maxLength>
		</dict:constraint>	
	</dict:constraintDescriptor>
	<dict:constraint key="single.line.text" id="constraint.single.line.text" serverSide="false">
		<!-- A basic single line of text, no embedded carraige returns or tabs limited to 255 -->
		<!-- old reg ex was "[A-Za-z0-9.-;;'&%$#@!]* -->
		<dict:minLength>1</dict:minLength>
		<dict:maxLength>5</dict:maxLength>
		<dict:validChars>
		<dict:value>regex:[2-4]*</dict:value>
		</dict:validChars>
	</dict:constraint>	
	<dict:constraint key="single" id="constraint.single" serverSide="false">
		<!-- Single valued (non-repeating) -->
		<!-- Used to override a repeating constraint -->
		<dict:maxOccurs>4</dict:maxOccurs>
	</dict:constraint>	
	
</beans>