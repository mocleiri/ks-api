    <bean id="KS-ActivityOfferingWrapper-InquiryView" parent="KSInquiryView"
          p:headerText="ActivityOffering Inquiry"
          p:dataObjectClassName="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper"
          p:viewHelperServiceClass="org.kuali.student.enrollment.class2.courseoffering.service.impl.ActivityOfferingWrapperInquirableImpl">
        <property name="additionalCssFiles">
            <list merge="true">
                <value>ks-enroll/css/activityOffering.css</value>
            </list>
        </property>
        <property name="additionalScriptFiles">
            <list merge="true">
                <value>ks-enroll/scripts/activity-offering.js</value>
            </list>
        </property>
        <property name="page.header">
            <ref bean="KSHeader_Empty" />
        </property>
        <property name="Items">
            <list>
                <bean parent="Uif-HeaderFour" p:headerText="@{dataObject.courseOfferingCode} - @{dataObject.courseOfferingTitle} (@{dataObject.credits} Credits)"/>
                <bean parent="Uif-HeaderThree" p:headerText="Edit @{dataObject.activityCode} (@{dataObject.abbreviatedCourseType})"/>
                <ref bean="ActivityOfferingEdit-MainPage-section1"/>
                <ref bean="ActivityOfferingEdit-MainPage-section2"/>
                <ref bean="ActivityOffering-Inquiry-DeliveryLogistic-Actuals"/>
                <ref bean="ActivityOfferingEdit-MainPage-PersonnelSection"/>
                <ref bean="ActivityOfferingEdit-MainPage-SeatPool"/>
                <ref bean="ActivityOfferingEdit-MainPage-Miscellaneous"/>
            </list>
        </property>
    </bean>
    <bean id="ActivityOffering-Inquiry-DeliveryLogistic-Actuals" parent="Uif-TableCollectionSection"
          p:headerText="Actual Delivery Logistics"
          p:collectionObjectClass="org.kuali.student.enrollment.class2.courseoffering.dto.ScheduleWrapper"
          p:propertyName="actualScheduleComponents"
          p:readOnly="true"
          p:layoutManager.renderSequenceField="false">
        <property name="layoutManager.richTable.render" value="false" />
        <property name="items">
            <list>
                <bean parent="Uif-InputField" p:label="TBA" p:propertyName="tbaUI" p:readOnly="true" p:control.size="5">
                    <property name="control">
                        <bean parent="Uif-TextControl"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:label="Days" p:propertyName="daysUI" p:readOnly="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="5"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:label="Start Time" p:propertyName="startTimeUI" p:readOnly="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="5"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:label="End Time" p:propertyName="endTimeUI" p:readOnly="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="5"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:label="Facility" p:propertyName="buildingName" p:readOnly="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="20"/>
                    </property>
                </bean>
                <bean parent="Uif-InputField" p:label="Room" p:propertyName="roomCode" p:readOnly="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="5"/>
                    </property>
                </bean>
                <!--<bean parent="Uif-InputField" p:label="Capacity" p:propertyName="roomCapacity" p:readOnly="true">
                    <property name="control">
                        <bean parent="Uif-TextControl" p:size="5"/>
                    </property>
                </bean>-->
                <bean parent="Uif-MessageField" p:messageText="F" p:label="Features">
                    <property name="toolTip">
                        <bean parent="Uif-TooltipFocus" p:onMouseHover="true" p:tooltipContent="N/A"/>
                    </property>
                </bean>
            </list>
        </property>
    </bean>
</beans>