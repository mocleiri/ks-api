    <bean id="OfferingInstructorWrapper" parent="OfferingInstructorWrapper-parentBean"/>

    <bean id="OfferingInstructorWrapper-parentBean" abstract="true" parent="DataObjectEntry">
        <property name="name" value="offeringInstructorWrapper"/>
        <property name="dataObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.OfferingInstructorWrapper"/>
        <property name="objectLabel" value="OfferingInstructor Wrapper"/>
        <property name="titleAttribute" value="offeringInstructorInfo.personId"/>
        <property name="primaryKeys">
            <list>
                <value>offeringInstructorInfo.personId</value>
            </list>
        </property>
    </bean>

    <bean id="ActivityOfferingWrapper" parent="ActivityOfferingWrapper-parentBean"/>

    <bean id="ActivityOfferingWrapper-parentBean" abstract="true" parent="DataObjectEntry">
        <property name="dataObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper"/>
        <property name="titleAttribute" value="aoInfo.id"/>
        <property name="attributes">
            <list>
            </list>
        </property>
        <property name="objectLabel" value="Activity Offering"/>
        <property name="primaryKeys">
            <list>
                <value>aoInfo.id</value>
            </list>
        </property>
    </bean>

    <bean id="ActivityOffering-MaintenanceView" parent="KSMaintenanceView" p:readOnly="@{document.newMaintainableObject.dataObject.readOnlyView}" p:entryPageId="ActivityOfferingEdit-MainPage">
        <property name="headerText" value="Edit Activity Offering"/>
        <property name="breadcrumbTitlePropertyName" value="aoInfo.activityCode"/>
        <property name="dataObjectClassName" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper"/>
        <property name="viewHelperServiceClass" value="org.kuali.student.enrollment.class2.courseoffering.service.impl.ActivityOfferingMaintainableImpl" />
        <property name="singlePageView" value="false"/>
        <property name="additionalCssFiles">
            <list merge="true">
                <value>ks-enroll/css/activityOffering.css</value>
            </list>
        </property>
        <property name="additionalScriptFiles">
            <list merge="true">
                <value>ks-enroll/scripts/activity-offering.js</value>
            </list>
        </property>
        <property name="items">
            <list>
                <ref bean="ActivityOfferingEdit-MainPage"/>
                <ref bean="ActivityOfferingEdit-SchedulePage"/>
            </list>
        </property>

    </bean>

    <bean id="ActivityOfferingMaintenanceDocument" parent="ActivityOfferingMaintenanceDocument-parentBean"/>

    <bean id="ActivityOfferingMaintenanceDocument-parentBean" abstract="true" parent="uifMaintenanceDocumentEntry">
        <property name="dataObjectClass" value="org.kuali.student.enrollment.class2.courseoffering.dto.ActivityOfferingWrapper"/>
        <property name="maintainableClass" value="org.kuali.student.enrollment.class2.courseoffering.service.impl.ActivityOfferingMaintainableImpl"/>
        <property name="businessRulesClass" value="org.kuali.student.enrollment.class2.courseoffering.rule.ActivityOfferingRule" />
        <property name="documentTypeName" value="ActivityOfferingInfoMaintenanceDocument"/>
        <property name="lockingKeys">
            <list>
                <value>aoInfo.id</value>
            </list>
        </property>
    </bean>

    <util:list id="aoInstructors-options">
        <bean parent="Uif-KeyLabelPair" p:key="" p:value="Select One..."/>
        <bean parent="Uif-KeyLabelPair" p:key="kuali.lpr.type.instructor.main" p:value="Instructor"/>
        <bean parent="Uif-KeyLabelPair" p:key="kuali.lpr.type.instructor.assistant" p:value="Teaching Assistant"/>
    </util:list>

    <util:list id="expMilestones-options">
        <bean parent="Uif-KeyLabelPair" p:key="Test1" p:value="Test1"/>
        <bean parent="Uif-KeyLabelPair" p:key="Test2" p:value="Test2"/>
        <bean parent="Uif-KeyLabelPair" p:key="Test3" p:value="Test3"/>
    </util:list>

    <util:list id="features-options">
        <bean parent="Uif-KeyLabelPair" p:value="Computers" p:key="kuali.resource.type.Computers"/>
        <bean parent="Uif-KeyLabelPair" p:value="  PCs" p:key="kuali.resource.type.PCs"/>
        <bean parent="Uif-KeyLabelPair" p:value="  Macs" p:key="kuali.resource.type.Macs"/>
        <bean parent="Uif-KeyLabelPair" p:value="  Ethernet" p:key="kuali.resource.type.Ethernet"/>
        <bean parent="Uif-KeyLabelPair" p:value="  Wifi" p:key="kuali.resource.type.Wifi"/>

        <bean parent="Uif-KeyLabelPair" p:value="Seating" p:key="kuali.resource.type.Seating"/>
        <bean parent="Uif-KeyLabelPair" p:value="  Fixed" p:key="kuali.resource.type.Fixed"/>
        <bean parent="Uif-KeyLabelPair" p:value="  Auditorium" p:key="kuali.resource.type.Auditorium"/>
        <bean parent="Uif-KeyLabelPair" p:value="  Lab" p:key="kuali.resource.type.Lab"/>
    </util:list>

</beans>
