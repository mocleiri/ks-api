<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dict="http://student.kuali.org/xsd/dictionary-extension"
	xsi:schemaLocation="
	http://student.kuali.org/xsd/dictionary-extension http://student.kuali.org/xsd/dictionary-extension/dictionary-extension.xsd
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	 ">


	<dict:fieldDescriptor id="baseString">
		<dict:dataType>string</dict:dataType>
		<dict:minLength>0</dict:minLength>
		<dict:maxLength>2000</dict:maxLength>
		<dict:validChars>ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-</dict:validChars>
		<dict:minOccurs>0</dict:minOccurs>
		<dict:maxOccurs>1</dict:maxOccurs>
		<dict:readOnly>false</dict:readOnly>
	</dict:fieldDescriptor>

	<dict:fieldDescriptor id="baseDate">
		<dict:dataType>date</dict:dataType>
		<dict:minLength>8</dict:minLength>
		<dict:maxLength>8</dict:maxLength>
		<dict:validChars>0123456789</dict:validChars>
		<dict:minOccurs>0</dict:minOccurs>
		<dict:maxOccurs>1</dict:maxOccurs>
		<dict:readOnly>false</dict:readOnly>
	</dict:fieldDescriptor>

	<dict:objectStructure key="orgInfo">
		<dict:type key="kuali.org.Program">
			<dict:state id="kuali.org.orgInfo.Program.active" key="active">
				<dict:field id="field1" key="desc">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>Description</dict:name>
						<dict:desc>Description of this Org.</dict:desc>
						<dict:enum key="lu.nextReviewPeriod">
							<dict:contextDescriptors>
								<dict:contextDescriptor type="Department">
									<dict:contextValueDescriptor>
										<dict:desc>Passes back a list of atpKeys associated with a
											particular org or department.</dict:desc>
										<dict:dataType>string</dict:dataType>
										<dict:minLength>1</dict:minLength>
										<dict:maxLength>16</dict:maxLength>
										<dict:validChars>ABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789-</dict:validChars>
										<dict:minOccurs>1</dict:minOccurs>
										<dict:maxOccurs>1</dict:maxOccurs>
										<dict:readOnly>false</dict:readOnly>
									</dict:contextValueDescriptor>
								</dict:contextDescriptor>
							</dict:contextDescriptors>
						</dict:enum>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="longName">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>Long Name</dict:name>
						<dict:desc>The longer name for this Org.</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="shortName">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>Short Name</dict:name>
						<dict:desc>The shorter name for this Org.</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="Alias">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>Alias</dict:name>
						<dict:desc>The alias for this Org.</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="effectiveDate">
					<dict:fieldDescriptor parent="baseDate">
						<dict:name>Effective Date</dict:name>
						<dict:desc>Date on which an org becomes active.</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="expirationDate">
					<dict:fieldDescriptor parent="baseDate">
						<dict:name>Expiration Date</dict:name>
						<dict:desc>Date on which an org becomes inactive.</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
			</dict:state>
			<dict:state key="proposed" parent="kuali.org.orgInfo.Program.active" />
		</dict:type>
		<dict:type key="kuali.org.College">
			<dict:state key="updated active" parent="kuali.org.orgInfo.Program.active" />
		</dict:type>
	</dict:objectStructure>
</beans>