<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="EnumerationContextValue" parent="EnumerationContextValue-parentBean" />

    <bean id="EnumerationContextValue-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass"
            value="org.kuali.student.core.enumerationmanagement.bo.EnumerationContextValue" />
        <property name="titleAttribute" value="contextId" />
        <property name="objectLabel" value="Context Enumeration Value" />
        <property name="lookupDefinition">
            <ref bean="EnumerationContextValue-lookupDefinition" />
        </property>
        <property name="inquiryDefinition">
            <ref bean="EnumerationContextValue-inquiryDefinition" />
        </property>
        <property name="attributes">
            <list>
                <ref bean="EnumerationContextValue-contextId" />
                <ref bean="EnumerationContextValue-enumerationValueId" />
                <ref bean="EnumerationContextValue-active" />
                <ref bean="EnumerationContextValue-enumerationContext.contextKey" />
                <ref bean="EnumerationContextValue-enumerationValue.code" />
            </list>
        </property>
    </bean>

    <bean id="EnumerationContextValue-contextId" parent="EnumerationContextValue-contextId-parentBean" />

    <bean id="EnumerationContextValue-contextId-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="ContextId" />
        <property name="maxLength" value="19" />
        <property name="validationPattern">
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30" />
        </property>
        <property name="name" value="contextId" />
        <property name="label" value="Context Id" />
    </bean>

    <bean id="EnumerationContextValue-enumerationValueId" parent="EnumerationContextValue-enumerationValueId-parentBean" />

    <bean id="EnumerationContextValue-enumerationValueId-parentBean" abstract="true"
        parent="AttributeDefinition">
        <property name="shortLabel" value="Enumeration Value Id" />
        <property name="maxLength" value="19" />
        <property name="validationPattern">
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30" />
        </property>
        <property name="name" value="enumerationValueId" />
        <property name="label" value="Enumerated Value Id" />
    </bean>

    <bean id="EnumerationContextValue-active" parent="EnumerationContextValue-active-parentBean" />

    <bean id="EnumerationContextValue-active-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="active" />
        <property name="maxLength" value="50" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern" />
        </property>
        <property name="control">
            <bean parent="CheckboxControlDefinition" />
        </property>
        <property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter" />
        <property name="summary" value="active" />
        <property name="name" value="active" />
        <property name="label" value="Active Indicator" />
    </bean>

    <bean id="EnumerationContextValue-enumerationContext.contextKey" parent="EnumerationContextValue-enumerationContext.contextKey-parentBean" />

    <bean id="EnumerationContextValue-enumerationContext.contextKey-parentBean" abstract="true"
        parent="AttributeDefinition">
        <property name="shortLabel" value="Context Key" />
        <property name="maxLength" value="255" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true" />
        </property>
        <property name="control">
            <bean parent="LookupReadonlyControlDefinition" />
        </property>
        <property name="name" value="enumerationContext.contextKey" />
        <property name="label" value="Context Key" />
    </bean>

    <bean id="EnumerationContextValue-enumerationValue.code" parent="EnumerationContextValue-enumerationValue.code-parentBean" />

    <bean id="EnumerationContextValue-enumerationValue.code-parentBean" abstract="true"
        parent="AttributeDefinition">
        <property name="shortLabel" value="Code" />
        <property name="maxLength" value="255" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true" />
        </property>
        <property name="control">
            <bean parent="LookupReadonlyControlDefinition" />
        </property>
        <property name="name" value="enumerationValue.code" />
        <property name="label" value="Code" />
    </bean>


    <bean id="EnumerationContextValue-lookupDefinition" parent="EnumerationContextValue-lookupDefinition-parentBean" />

    <bean id="EnumerationContextValue-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="contextId" />
                <bean parent="FieldDefinition" p:attributeName="enumerationValueId" />
                <bean parent="FieldDefinition" p:attributeName="active" />
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="contextId" />
                <bean parent="FieldDefinition" p:attributeName="enumerationValueId" />
                <bean parent="FieldDefinition" p:attributeName="active" />
            </list>
        </property>
        <property name="title" value="Context Enumeration Value Lookup" />
    </bean>

    <bean id="EnumerationContextValue-inquiryDefinition" parent="EnumerationContextValue-inquiryDefinition-parentBean" />

    <bean id="EnumerationContextValue-inquiryDefinition-parentBean" abstract="true"
        parent="InquiryDefinition">
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Context Enumeration Value" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="contextId" />
                            <bean parent="FieldDefinition" p:attributeName="enumerationValueId" />
                            <bean parent="FieldDefinition" p:attributeName="active" />
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

</beans>
    