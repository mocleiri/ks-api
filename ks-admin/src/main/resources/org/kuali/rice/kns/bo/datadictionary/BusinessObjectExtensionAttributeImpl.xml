  <bean id="BusinessObjectExtensionAttributeImpl" parent="BusinessObjectExtensionAttributeImpl-parentBean"/>

  <bean id="BusinessObjectExtensionAttributeImpl-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.kns.bo.BusinessObjectExtensionAttributeImpl"/>
    <property name="attributes">
      <list>
        <ref bean="BusinessObjectExtensionAttributeImpl-id"/>
        <ref bean="BusinessObjectExtensionAttributeImpl-name"/>
        <ref bean="BusinessObjectExtensionAttributeImpl-value"/>
        <ref bean="BusinessObjectExtensionAttributeImpl-ownerObjectId"/>
      </list>
    </property>
    <property name="objectLabel" value="Business Object Extension Attribute"/>
  </bean>

<!-- Attribute Definitions -->


  <bean id="BusinessObjectExtensionAttributeImpl-id" parent="BusinessObjectExtensionAttributeImpl-id-parentBean"/>

  <bean id="BusinessObjectExtensionAttributeImpl-id-parentBean" abstract="true" parent="AttributeReferenceKualiType-id">
    <property name="summary" value="Unique id for extension attribute"/>
    <property name="name" value="id"/>
    <property name="label" value="Attribute Id"/>
    <property name="description" value="Unique id for extension attribute"/>
  </bean>
  
  <bean id="BusinessObjectExtensionAttributeImpl-name" parent="BusinessObjectExtensionAttributeImpl-name-parentBean"/>

  <bean id="BusinessObjectExtensionAttributeImpl-name-parentBean" abstract="true" parent="AttributeReferenceKualiType-name">
    <property name="summary" value="Name for the Attribute"/>
    <property name="name" value="name"/>
    <property name="label" value="Attribute Name"/>
    <property name="description" value="Name for the Attribute"/>
  </bean>
  
  <bean id="BusinessObjectExtensionAttributeImpl-value" parent="BusinessObjectExtensionAttributeImpl-value-parentBean"/>

  <bean id="BusinessObjectExtensionAttributeImpl-value-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="forceUppercase" value="false"/>
    <property name="shortLabel" value="Val"/>
    <property name="maxLength" value="2000"/>
    <property name="validationPattern">
      <bean parent="AnyCharacterValidationPattern" p:allowWhitespace="true"/>
    </property>
    <property name="control">
      <bean parent="TextControlDefinition" p:size="100"/>
    </property>
    <property name="summary" value="Value for the Attribute"/>
    <property name="name" value="value"/>
    <property name="label" value="Attribute Value"/>
    <property name="description" value="Value for the Attribute"/>
  </bean>
  
  <bean id="BusinessObjectExtensionAttributeImpl-ownerObjectId" parent="BusinessObjectExtensionAttributeImpl-ownerObjectId-parentBean"/>

  <bean id="BusinessObjectExtensionAttributeImpl-ownerObjectId-parentBean" abstract="true" parent="AttributeReferenceDummy-objectId">
    <property name="shortLabel" value="Owner"/>
    <property name="summary" value="Object Id for the owning record"/>
    <property name="name" value="ownerObjectId"/>
    <property name="label" value="Owner Id"/>
    <property name="description" value="Object Id for the owning record"/>
  </bean>
</beans>
