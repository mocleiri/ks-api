<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="
http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">

	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<import resource="classpath:lum-context.xml" />

	<jaxws:endpoint id="luService"
		implementorClass="edu.umd.ks.poc.lum.lu.service.impl.LuServiceImpl"
		implementor="#luServiceImpl" address="/LuService" />

	<jaxws:endpoint id="scatService"
		implementorClass="edu.umd.ks.poc.lum.scat.service.impl.ScatServiceImpl"
		implementor="#scatServiceImpl" address="/ScatService" />
</beans>