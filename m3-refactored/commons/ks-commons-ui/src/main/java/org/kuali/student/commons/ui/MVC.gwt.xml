<module>

	<!-- Inherit the core Web Toolkit stuff.                  -->
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="org.kuali.student.commons.ui.Messages"/>
	<inherits name="org.kuali.student.commons.ui.Utilities"/>
	<inherits name="org.kuali.student.commons.ui.ViewMetaData"/>
	<inherits name="org.kuali.student.commons.ui.PropertyChangeSupport"/>


	<!-- Specify the app entry point class.                   -->
	<entry-point class="org.kuali.student.commons.ui.mvc.client.MVC"/>
    <source path="mvc/client" />
    <generate-with class="org.kuali.student.commons.ui.mvc.rebind.MetadataGenerator" >
        <when-type-assignable class="org.kuali.student.commons.ui.mvc.client.HasMetadata"/>
    </generate-with> 
</module>
