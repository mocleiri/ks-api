<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dict="http://student.kuali.org/xsd/dictionary-extension"
	xsi:schemaLocation="
	http://student.kuali.org/xsd/dictionary-extension http://student.kuali.org/xsd/dictionary-extension/dictionary-extension.xsd
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	 ">
	<dict:fieldDescriptor id="parentFieldDescriptor">
		<dict:name>CLU Identifier</dict:name>
		<dict:desc>
			Unique identifier for a Canonical Learning Unit(CLU).
		</dict:desc>
		<dict:dataType>string</dict:dataType>
		<dict:minLength>36</dict:minLength>
		<dict:maxLength>36</dict:maxLength>
		<dict:validChars>ABCDEF0123456789-</dict:validChars>
		<dict:invalidChars></dict:invalidChars>
		<dict:readOnly>true</dict:readOnly>
	</dict:fieldDescriptor>

	<dict:fieldDescriptor id="childFieldDescriptor"
		parent="parentFieldDescriptor">
		<dict:name>Child CLU Identifier</dict:name>
	</dict:fieldDescriptor>

	<dict:fieldDescriptor id="baseString">
		<dict:dataType>string</dict:dataType>
		<dict:minLength>0</dict:minLength>
		<dict:maxLength>2000</dict:maxLength>
		<!-- The space character is important -->
		<dict:validChars>
			ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789
			-.,
		</dict:validChars>
		<dict:readOnly>false</dict:readOnly>
	</dict:fieldDescriptor>

	<dict:objectStructure key="cluIdentifierInfo" id="cluIdentifierInfo">
		<dict:type key="course">
			<dict:state key="proposed">
				<dict:field key="code" minOccurs="0" maxOccurs="1">
					<dict:fieldDescriptor>
						<dict:name>Clu Code</dict:name>
						<dict:desc>
							Composite identification code for this clu
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>0</dict:minLength>
						<dict:maxLength>50</dict:maxLength>
						<!-- The space character is important -->
						<dict:validChars>
							ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789
							-.,
						</dict:validChars>
						<dict:readOnly>false</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="shortName" minOccurs="1"
					maxOccurs="1">
					<dict:fieldDescriptor>
						<dict:name>Short Name</dict:name>
						<dict:desc>
							Abbreviated name of the CLU, commonly used
							on transcripts
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>0</dict:minLength>
						<dict:maxLength>50</dict:maxLength>
						<!-- The space character is important -->
						<dict:validChars>
							ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789
							-.,
						</dict:validChars>
						<dict:readOnly>false</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="longName" minOccurs="1"
					maxOccurs="1">
					<dict:fieldDescriptor>
						<dict:name>Long Name</dict:name>
						<dict:desc>
							Full name of the CLU, commonly used on
							catalogues
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>0</dict:minLength>
						<dict:maxLength>2000</dict:maxLength>
						<!-- The space character is important -->
						<dict:validChars>
							ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789
							-.,
						</dict:validChars>
						<dict:readOnly>false</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="level" minOccurs="1" maxOccurs="1">
					<dict:fieldDescriptor>
						<dict:name>Level</dict:name>
						<dict:desc>
							A code that indicates whether this is
							introductory, advanced, etc.
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>0</dict:minLength>
						<dict:maxLength>2000</dict:maxLength>
						<!-- The space character is important -->
						<dict:validChars>
							ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789
							-.,
						</dict:validChars>
						<dict:readOnly>false</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
			</dict:state>
		</dict:type>
	</dict:objectStructure>

	<dict:objectStructure key="cluInfo">
		<dict:type key="luType.shell.course">
			<dict:state key="proposed">
				<dict:field key="cluId2">
					<dict:fieldDescriptor>
						<ref bean="childFieldDescriptor" />
					</dict:fieldDescriptor>
					<dict:selector>false</dict:selector>
				</dict:field>
				<dict:field key="cluId">
					<dict:fieldDescriptor>
						<dict:name>CLU Identifier</dict:name>
						<dict:desc>
							Unique identifier for a Canonical Learning
							Unit (CLU).
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>1</dict:minLength>
						<dict:maxLength>36</dict:maxLength>
						<dict:validChars>
							ABCDEF0123456789-
						</dict:validChars>
						<dict:readOnly>true</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>

				<dict:field key="cluCategory">
					<dict:fieldDescriptor>
						<dict:name>CLU Category</dict:name>
						<dict:desc>
							Allows for categorization of CLUs.
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>1</dict:minLength>
						<dict:maxLength>1</dict:maxLength>
						<dict:enum key="lu.cluCategories">
							<dict:contextDescriptors>
								<dict:contextDescriptor
									type="default">
									<dict:contextValueDescriptor>
										<dict:desc>
											Passes back an unfiltered
											list. Context value should
											be null or an empty string.
										</dict:desc>
									</dict:contextValueDescriptor>
								</dict:contextDescriptor>
								<dict:contextDescriptor type="org">
									<dict:contextValueDescriptor>
										<dict:desc>
											Passes back a list of clu
											categories associated with a
											particular organization.
											This is a bad example.
										</dict:desc>
									</dict:contextValueDescriptor>
								</dict:contextDescriptor>
							</dict:contextDescriptors>
						</dict:enum>
					</dict:fieldDescriptor>
					<dict:selector>true</dict:selector>
				</dict:field>
			</dict:state>

			<dict:state key="active">
				<dict:field key="cluId" minOccurs="1" maxOccurs="1">
					<dict:fieldDescriptor>
						<dict:name>CLU Identifier</dict:name>
						<dict:desc>
							Unique identifier for a Canonical Learning
							Unit(CLU).
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>36</dict:minLength>
						<dict:maxLength>36</dict:maxLength>
						<dict:validChars>
							ABCDEF0123456789-
						</dict:validChars>
						<dict:readOnly>true</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="cluCategory">
					<dict:fieldDescriptor>
						<dict:name>CLU Category</dict:name>
						<dict:desc>
							Allows for categorization of CLUs.
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>1</dict:minLength>
						<dict:maxLength>1</dict:maxLength>
					</dict:fieldDescriptor>
					<dict:selector>true</dict:selector>
				</dict:field>
			</dict:state>
		</dict:type>
		<dict:type key="program">
			<dict:state key="proposed">
				<dict:field key="cluId">
					<dict:fieldDescriptor>
						<dict:name>CLU Identifier</dict:name>
						<dict:desc>
							Unique identifier for a Canonical Learning
							Unit (CLU).
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>36</dict:minLength>
						<dict:maxLength>36</dict:maxLength>
						<dict:validChars>
							ABCDEF0123456789-
						</dict:validChars>
						<dict:invalidChars></dict:invalidChars>
						<dict:readOnly>true</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="cluCategory">
					<dict:fieldDescriptor>
						<dict:name>CLU Category</dict:name>
						<dict:desc>
							Allows for categorization of CLUs.
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>1</dict:minLength>
						<dict:maxLength>1</dict:maxLength>
					</dict:fieldDescriptor>
					<dict:selector>true</dict:selector>
				</dict:field>
			</dict:state>
			<dict:state key="active">
				<dict:field key="cluId" minOccurs="1" maxOccurs="1">
					<dict:fieldDescriptor>
						<dict:name>CLU Identifier</dict:name>
						<dict:desc>
							Unique identifier for a Canonical Learning
							Unit (CLU).
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>36</dict:minLength>
						<dict:maxLength>36</dict:maxLength>
						<dict:validChars>
							ABCDEF0123456789-
						</dict:validChars>
						<dict:readOnly>true</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="cluCategory">
					<dict:fieldDescriptor>
						<dict:name>CLU Category</dict:name>
						<dict:desc>
							Allows for categorization of CLUs.
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>1</dict:minLength>
						<dict:maxLength>1</dict:maxLength>
					</dict:fieldDescriptor>
					<dict:selector>true</dict:selector>
				</dict:field>
			</dict:state>
		</dict:type>
	</dict:objectStructure>

	<dict:objectStructure key="proposalInfo">
		<dict:type key="luType.shell.course">
			<dict:state key="proposed">
				<dict:field key="id">
					<dict:fieldDescriptor>
						<dict:name>Proposal Identifier</dict:name>
						<dict:desc>
							Unique identifier for a Proposal.
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>1</dict:minLength>
						<dict:maxLength>36</dict:maxLength>
						<dict:validChars>
							ABCDEF0123456789-
						</dict:validChars>
						<dict:readOnly>true</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="name">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>Proposal Name</dict:name>
						<dict:desc>Name of this proposal</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="desc">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>Description</dict:name>
						<dict:desc>
							Description of this proposal
						</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="proposerPersonId">
					<dict:fieldDescriptor>
						<dict:name>Description</dict:name>
						<dict:desc>
							Description of this proposal
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>1</dict:minLength>
						<dict:maxLength>36</dict:maxLength>
						<dict:validChars>
							ABCDEF0123456789-
						</dict:validChars>
						<dict:readOnly>true</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="proposerOrgId">
					<dict:fieldDescriptor>
						<dict:name>Description</dict:name>
						<dict:desc>
							Description of this proposal
						</dict:desc>
						<dict:dataType>string</dict:dataType>
						<dict:minLength>1</dict:minLength>
						<dict:maxLength>36</dict:maxLength>
						<dict:validChars>
							ABCDEF0123456789-
						</dict:validChars>
						<dict:readOnly>true</dict:readOnly>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="rationale">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>Rationale</dict:name>
						<dict:desc>
							Rational for this proposal
						</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="type">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>Type</dict:name>
						<dict:desc>Type of this proposal</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
				<dict:field key="state">
					<dict:fieldDescriptor parent="baseString">
						<dict:name>State</dict:name>
						<dict:desc>State of this proposal</dict:desc>
					</dict:fieldDescriptor>
				</dict:field>
			</dict:state>
		</dict:type>
	</dict:objectStructure>



</beans>