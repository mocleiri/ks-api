<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
          http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

    <context:annotation-config/>

    <bean id="propositionTranslator" class="org.kuali.student.krms.naturallanguage.translators.PropositionTranslator">
        <property name="contextRegistry" ref="contextRegistryReqComponent" />
    </bean>

    <bean id="basicContext" class="org.kuali.student.krms.naturallanguage.config.context.BasicContextImpl">
    </bean>
    <bean id="luContext" class="org.kuali.student.krms.naturallanguage.config.context.CluContextImpl">
        <!--property name="CluService" ref="cluServiceImpl" /-->
    </bean>
    <bean id="lrcContext" class="org.kuali.student.krms.naturallanguage.config.context.LrcContextImpl">
        <!--property name="lrcService" ref="lrcServiceImpl"/>
        <property name="typeService" ref="typeServiceImpl"/-->
    </bean>
    <bean id="gpaContext" class="org.kuali.student.krms.naturallanguage.config.context.GpaContextImpl">
    </bean>
    <bean id="personContext" class="org.kuali.student.krms.naturallanguage.config.context.PersonContextImpl">
    </bean>
    <bean id="orgContext" class="org.kuali.student.krms.naturallanguage.config.context.OrganizationContextImpl">
        <!--property name="organizationService" ref="orgServiceImpl" /-->
    </bean>
    <bean id="atpContext" class="org.kuali.student.krms.naturallanguage.config.context.AtpContextImpl">
        <!--property name="atpService" ref="atpServiceImpl" />
        <property name="typeService" ref="typeServiceImpl"/-->
    </bean>

    <bean id="contextRegistryReqComponent"
          class="org.kuali.student.krms.naturallanguage.ContextRegistry">
        <constructor-arg>
            <map>
                <entry key="kuali.reqComponent.type.test">
                    <list>
                        <ref local="orgContext" />
                    </list>
                </entry>
            </map>
        </constructor-arg>
    </bean>

</beans>