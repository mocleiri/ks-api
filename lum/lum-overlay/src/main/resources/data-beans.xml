<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<!-- Scat Data -->
	
	<bean id="scatBeanPreloadList"
		class="org.springframework.beans.factory.config.ListFactoryBean">
		<property name="sourceList">
			<list>
				<ref bean="departmentsScatTable" />
				<ref bean="mathScat" />
				<ref bean="englishScat" />
			</list>
		</property>
	</bean>
	
	<bean id="departmentsScatTable"
		class="edu.umd.ks.poc.lum.scat.entity.ScatTable">
		<property name="tableId"
			value="11223344-1122-1122-1111-000000000000" />
		<property name="tableDescription" value="DEPARTMENTS" />
	</bean>
	<bean id="mathScat"
		class="edu.umd.ks.poc.lum.scat.entity.Scat">
		<property name="id"
			value="11223344-1122-1122-1111-000000000001" />
		<property name="code" value="MATH"/>
		<property name="shortDesc" value="Math"/>
		<property name="table" ref="departmentsScatTable"/>
	</bean>
	<bean id="englishScat"
		class="edu.umd.ks.poc.lum.scat.entity.Scat">
		<property name="id"
			value="11223344-1122-1122-1111-000000000002" />
		<property name="code" value="ENGL"/>
		<property name="shortDesc" value="English"/>
		<property name="table" ref="departmentsScatTable"/>
	</bean>
	
</beans>