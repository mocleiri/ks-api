<!-- Business Object Lookup Definition -->
  <bean id="CourseTransient-lookupDefinition" parent="CourseTransient-lookupDefinition-parentBean" />

  <bean id="CourseTransient-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Course Transient Lookup" />
    <property name="lookupableID" value="courseTransientLookupable" />

    <property name="defaultSort" >
      <bean parent="SortDefinition">
      </bean>
    </property>
    <property name="lookupFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="courseTitle" /> 
        <bean parent="FieldDefinition"
              p:attributeName="courseCode" />	                             
        <bean parent="FieldDefinition"
              p:attributeName="subjectArea" />           
      </list>
    </property>
    <property name="resultFields" >
      <list>
        <bean parent="FieldDefinition"
              p:attributeName="courseId" />
        <bean parent="FieldDefinition"
              p:attributeName="courseTitle" /> 
        <bean parent="FieldDefinition"
              p:attributeName="courseCode" />	               
        <bean parent="FieldDefinition"
              p:attributeName="subjectArea" />                              
      </list>
    </property>
  </bean>       
</beans>