  <bean id="CourseTransient" parent="CourseTransient-parentBean"/>

  <bean id="CourseTransient-parentBean" abstract="true" parent="BusinessObjectEntry">
    <property name="businessObjectClass" value="org.kuali.rice.student.bo.CourseTransient"/>
    <property name="lookupDefinition" >
      <ref bean="CourseTransient-lookupDefinition" />
    </property>
    <property name="titleAttribute" value="courseId" />
    <property name="objectLabel" value="Course Transient"/>
    <property name="attributes" >
      <list>
        <ref bean="CourseTransient-courseId" />
        <ref bean="CourseTransient-courseTitle" />
        <ref bean="CourseTransient-courseCode" />
        <ref bean="CourseTransient-subjectArea" />
       </list>
    </property>
  </bean>

<!-- Attribute Definitions -->
       
  <bean id="CourseTransient-courseId" parent="CourseTransient-courseId-parentBean" />

  <bean id="CourseTransient-courseId-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="courseId" />
    <property name="label" value="Course ID" />
    <property name="shortLabel" value="Course ID" />
    <property name="maxLength" value="100" />
    <property name="required" value="true"/>
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
  </bean>

  <bean id="CourseTransient-courseTitle" parent="CourseTransient-courseTitle-parentBean" />

  <bean id="CourseTransient-courseTitle-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="courseTitle" />
    <property name="label" value="Course Title" />
    <property name="shortLabel" value="Title" />
    <property name="maxLength" value="100" />
    <property name="required" value="true"/>
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
  </bean>
  
  <bean id="CourseTransient-courseCode" parent="CourseTransient-courseCode-parentBean" />

  <bean id="CourseTransient-courseCode-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="courseCode" />
    <property name="label" value="Course Code" />
    <property name="shortLabel" value="Code" />
    <property name="maxLength" value="100" />
    <property name="required" value="true"/>
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="false" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
  </bean>
  
  <bean id="CourseTransient-subjectArea" parent="CourseTransient-subjectArea-parentBean" />

  <bean id="CourseTransient-subjectArea-parentBean" abstract="true" parent="AttributeDefinition">
    <property name="name" value="subjectArea" />
    <property name="label" value="Division / Subject Area" />
    <property name="shortLabel" value="Division" />
    <property name="maxLength" value="100" />
    <property name="required" value="true"/>
    <property name="validationPattern" >
      <bean parent="AnyCharacterValidationPattern"
            p:allowWhitespace="true" />
    </property>
    <property name="control" >
      <bean parent="TextControlDefinition"
            p:size="42" />
    </property>
  </bean>

    
