<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.kuali.student.test</groupId>
    <artifactId>test</artifactId>
    <packaging>pom</packaging>
    <name>KS Testing Pom</name>
    <url>http://driver-1.ks.kuali.net/docs</url>
    <description>Kuali Student Testing Project</description>
    <version>1.0.0</version>
    <modules>
        <!--<module>driver</module>-->
        <module>performance</module>
        <!--<module>ui</module>-->
        <!--<module>web-services</module>-->
    </modules>
    <build>
		<pluginManagement>
			<plugins>
		      	<plugin>
        			<artifactId>maven-release-plugin</artifactId>
				    <configuration>
					    <username>${maven.release.username}</username>
				        <password>${maven.release.password}</password>
			    	    <arguments>-Dmaven.test.skip=true</arguments>
			        	<preparationGoals>clean install</preparationGoals>
				        <goals>clean deploy</goals>
				    </configuration>
      			</plugin>
			</plugins>
		</pluginManagement>
	</build>
	<reporting>
    	<plugins>
      		<plugin>
        		<artifactId>maven-plugin-plugin</artifactId>
        		<version>2.5</version>
			</plugin>
			<plugin>
        		<groupId>org.apache.maven.plugins</groupId>
        		<artifactId>maven-project-info-reports-plugin</artifactId>
        		<version>2.1.2</version>
      		</plugin>
    	</plugins>
	</reporting>
    <scm>
      <connection>scm:svn:https://test.kuali.org/svn/student/deploymentlab/tags/tsung-trunk-1.0.0</connection>
      <developerConnection>scm:svn:https://test.kuali.org/svn/student/deploymentlab/tags/tsung-trunk-1.0.0</developerConnection>
      <url>https://test.kuali.org/svn/student/deploymentlab/tags/tsung-trunk-1.0.0</url>
    </scm>
    <distributionManagement>
      <site>
        <id>test.kuali.org/student</id>
        <url>file://C:/docs</url>
      </site>
      <repository>
        <uniqueVersion>true</uniqueVersion>
        <id>internal</id>
        <name>Kuali Student Repository</name>
        <url>file://C:/maven2</url>
        <layout>default</layout>
      </repository>
    </distributionManagement>
</project>