<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.kuali.student.test</groupId>
  <artifactId>test</artifactId>
  <packaging>pom</packaging>
  <name>Kuali Student Testing</name>
  <url>http://driver-1.ks.kuali.net/</url>
  <description>Kuali Student Testing</description>
  <version>1.0.0</version>
  <modules>
    <!--<module>driver</module>-->
    <module>performance</module>
    <module>ui</module>
    <module>ws</module>
  </modules>
  <licenses>
    <license>
      <name>Educational Community License, Version 1.0</name>
      <url>LICENSE.txt</url>
    </license>
  </licenses>  
  <issueManagement>
    <system>JIRA</system>
    <url>https://test.kuali.org/jira/browse/KSLAB</url>
  </issueManagement>       
  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <artifactId>maven-release-plugin</artifactId>
          <configuration>
            <username>${maven.release.username}</username>
            <password>${maven.release.password}</password>
            <arguments>-Dmaven.test.skip=true</arguments>
            <preparationGoals>clean install</preparationGoals>
            <goals>clean deploy</goals>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>
  <reporting>
    <plugins>
      <plugin>
        <artifactId>maven-plugin-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-project-info-reports-plugin</artifactId>
      </plugin>
    </plugins>
  </reporting>
  <scm>
    <connection>scm:svn:https://test.kuali.org/svn/student/deploymentlab/tags/test-tsung-trunk-1.0.0</connection>
    <developerConnection>scm:svn:https://test.kuali.org/svn/student/deploymentlab/tags/test-tsung-trunk-1.0.0</developerConnection>
    <url>https://test.kuali.org/svn/student/deploymentlab/tags/test-tsung-trunk-1.0.0</url>
  </scm>
  <distributionManagement>
    <site>
      <id>driver-1.ks.kuali.net</id>
      <url>file://C:/site</url>
    </site>
    <repository>
      <id>driver-1.ks.kuali.net</id>
      <uniqueVersion>true</uniqueVersion>
      <name>Kuali Student Repository</name>
      <url>file://C:/maven2</url>
      <layout>default</layout>
    </repository>
  </distributionManagement>
</project>