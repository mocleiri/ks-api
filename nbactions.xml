<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>build-with-dependencies</actionName>
            <goals>
                <goal>install</goal>
            </goals>
            <basedir>..</basedir>
            <reactor>also-make</reactor>
        </action>
        <action>
            <actionName>CUSTOM-wsdl2java</actionName>
            <displayName>wsdl2java</displayName>
            <goals>
                <goal>generate-sources</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-install no test</actionName>
            <displayName>install no test</displayName>
            <goals>
                <goal>install</goal>
            </goals>
            <properties>
                <maven.test.skip>true</maven.test.skip>
            </properties>
        </action>
        <action>
            <actionName>CUSTOM-show debug</actionName>
            <displayName>show debug</displayName>
            <goals>
                <goal>install</goal>
            </goals>
        </action>
        <action>
            <actionName>CUSTOM-one jar</actionName>
            <displayName>one jar</displayName>
            <goals>
                <goal>package</goal>
            </goals>
            <properties>
                <maven.test.skip>true</maven.test.skip>
            </properties>
        </action>
        
    </actions>
