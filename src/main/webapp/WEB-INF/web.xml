<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>spring</servlet-name>
		<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
		<load-on-startup>2</load-on-startup>
		<init-param>
			<param-name>contextConfigLocation</param-name>
			<param-value>classpath:gwt-context.xml</param-value>
		</init-param>		
	</servlet>

	<servlet>
		<servlet-name>messageServiceGwt</servlet-name>
		<servlet-class>org.kuali.student.common.ui.server.messages.MessagesService</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
    <servlet>
        <servlet-name>remoteLoggingService</servlet-name>
        <servlet-class>com.google.gwt.gen2.logging.server.RemoteLoggingService</servlet-class>
    </servlet>
    <servlet-mapping>
         <servlet-name>remoteLoggingService</servlet-name>
          <url-pattern>/org.kuali.student.ui.kitchensink.KitchenSink/logging</url-pattern>
    </servlet-mapping>
	<servlet-mapping>
		<servlet-name>spring</servlet-name>
		<url-pattern>/org.kuali.student.ui.kitchensink.KitchenSink/MessageService</url-pattern>
	</servlet-mapping>
</web-app>