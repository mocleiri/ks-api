<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p" xmlns:dd="http://rice.kuali.org/dd" xsi:schemaLocation="http://www.springframework.org/schema/beans         http://www.springframework.org/schema/beans/spring-beans-2.0.xsd         http://rice.kuali.org/dd         http://rice.kuali.org/dd/dd.xsd">

  <bean id="LearningUnitMaintenanceDocument" parent="LearningUnitMaintenanceDocument-parentBean"/>

  <bean id="LearningUnitMaintenanceDocument-parentBean" abstract="true" parent="MaintenanceDocumentEntry">
    <property name="businessObjectClass" value="org.kuali.rice.student.bo.LearningUnit"/>
    <property name="maintainableClass" value="org.kuali.rice.kns.maintenance.KualiMaintainableImpl"/>
    <property name="maintainableSections">
      <list>
        <ref bean="LearningUnitMaintenanceDocument-EditLearningUnit"/>
<!--
        <ref bean="LearningUnitMaintenanceDocument-Departments"/>
-->
      </list>
    </property>
    <property name="lockingKeys">
      <list>
        <value>id</value>
      </list>
    </property>

    <property name="documentTypeName" value="LearningUnitDocument"/>
    <property name="documentAuthorizerClass" value="org.kuali.rice.kns.document.authorization.MaintenanceDocumentAuthorizerBase"/>
    <property name="workflowAttributes">
    	<ref bean="LearningUnitMaintenanceDocument-workflowAttributes"/>
    </property>
  </bean>

<!-- Maintenance Section Definitions -->

  <bean id="LearningUnitMaintenanceDocument-EditLearningUnit" parent="LearningUnitMaintenanceDocument-EditLearningUnit-parentBean"/>

  <bean id="LearningUnitMaintenanceDocument-EditLearningUnit-parentBean" abstract="true" parent="MaintainableSectionDefinition">
    <property name="id" value="Edit Learning Unit"/>
    <property name="title" value="Edit Learning Unit"/>
    <property name="maintainableItems">
      <list>
        <bean parent="MaintainableFieldDefinition" p:name="id"/>
        <bean parent="MaintainableFieldDefinition" p:name="state" p:required="true"/>
        <bean parent="MaintainableFieldDefinition" p:name="title" p:required="true"/>
      </list>
    </property>
    <property name="allowsNewOrCopy" value="true"/>
  </bean>
