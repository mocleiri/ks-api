<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:p="http://www.springframework.org/schema/p"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

    <bean id="EnumerationValue" parent="EnumerationValue-parentBean" />

    <bean id="EnumerationValue-parentBean" abstract="true" parent="BusinessObjectEntry">
        <property name="businessObjectClass" value="org.kuali.student.core.enumerationmanagement.bo.EnumerationValue" />
        <property name="titleAttribute" value="code" />
        <property name="objectLabel" value="Enumeration Value" />
        <property name="lookupDefinition">
            <ref bean="EnumerationValue-lookupDefinition" />
        </property>
        <property name="inquiryDefinition">
            <ref bean="EnumerationValue-inquiryDefinition" />
        </property>
        <property name="collections">
            <list>
                <bean parent="CollectionDefinition" p:name="enumerationContextValueList"
                    p:shortLabel="Contexts" p:summary="Contexts" p:label="Enumeration Contexts"
                    p:description="Contexts this Enumerated Value is a member of." />
            </list>
        </property>
        <property name="attributes">
            <list>
                <ref bean="EnumerationValue-id" />
                <ref bean="EnumerationValue-enumerationTableId" />
                <ref bean="EnumerationValue-code" />
                <ref bean="EnumerationValue-abbrevValue" />
                <ref bean="EnumerationValue-value" />
                <ref bean="EnumerationValue-sortKey" />
                <ref bean="EnumerationValue-active" />
                <ref bean="EnumerationValue-effectiveDate" />
                <ref bean="EnumerationValue-expirationDate" />
            </list>
        </property>
    </bean>

    <bean id="EnumerationValue-id" parent="EnumerationValue-id-parentBean" />

    <bean id="EnumerationValue-id-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Id" />
        <property name="maxLength" value="19" />
        <property name="validationPattern">
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30" />
        </property>
        <property name="name" value="id" />
        <property name="label" value="Id" />
    </bean>

    <bean id="EnumerationValue-enumerationTableId" parent="EnumerationValue-enumerationTableId-parentBean" />

    <bean id="EnumerationValue-enumerationTableId-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Enumeration Table Id" />
        <property name="maxLength" value="255" />
        <property name="validationPattern">
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30" />
        </property>
        <property name="name" value="enumerationTableId" />
        <property name="label" value="Enumeration Table Id" />
    </bean>

    <bean id="EnumerationValue-code" parent="EnumerationValue-code-parentBean" />

    <bean id="EnumerationValue-code-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Code" />
        <property name="maxLength" value="255" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="true" />
            </bean>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="50" />
        </property>
        <property name="name" value="code" />
        <property name="label" value="Enumeration Code" />
    </bean>

    <bean id="EnumerationValue-abbrevValue" parent="EnumerationValue-abbrevValue-parentBean" />

    <bean id="EnumerationValue-abbrevValue-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Abbreviated Value" />
        <property name="maxLength" value="255" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="true" />
            </bean>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="50" />
        </property>
        <property name="name" value="abbrevValue" />
        <property name="label" value="Abbreviated Value" />
    </bean>

    <bean id="EnumerationValue-value" parent="EnumerationValue-value-parentBean" />

    <bean id="EnumerationValue-value-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Value" />
        <property name="maxLength" value="255" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern">
                <property name="allowWhitespace" value="true" />
            </bean>
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="50" />
        </property>
        <property name="name" value="value" />
        <property name="label" value="Value" />
    </bean>

    <bean id="EnumerationValue-active" parent="EnumerationValue-active-parentBean" />

    <bean id="EnumerationValue-active-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="active" />
        <property name="maxLength" value="50" />
        <property name="validationPattern">
            <bean parent="AnyCharacterValidationPattern" />
        </property>
        <property name="control">
            <bean parent="CheckboxControlDefinition" />
        </property>
        <property name="formatterClass" value="org.kuali.rice.kns.web.format.BooleanFormatter" />
        <property name="summary" value="active" />
        <property name="name" value="active" />
        <property name="label" value="Active Indicator" />
    </bean>

    <bean id="EnumerationValue-effectiveDate" parent="EnumerationValue-effectiveDate-parentBean" />

    <bean id="EnumerationValue-effectiveDate-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Effective Date" />
        <property name="maxLength" value="10" />
        <property name="validationPattern">
            <bean parent="DateValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
        </property>
        <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter" />
        <property name="name" value="effectiveDate" />
        <property name="label" value="Effective Date" />
    </bean>

    <bean id="EnumerationValue-expirationDate" parent="EnumerationValue-expirationDate-parentBean" />

    <bean id="EnumerationValue-expirationDate-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Expiration Date" />
        <property name="maxLength" value="10" />
        <property name="validationPattern">
            <bean parent="DateValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="10" p:datePicker="true" />
        </property>
        <property name="formatterClass" value="org.kuali.rice.kns.web.format.DateFormatter" />
        <property name="name" value="expirationDate" />
        <property name="label" value="Expiration Date" />
    </bean>

    <bean id="EnumerationValue-sortKey" parent="EnumerationValue-sortKey-parentBean" />

    <bean id="EnumerationValue-sortKey-parentBean" abstract="true" parent="AttributeDefinition">
        <property name="shortLabel" value="Sort Key" />
        <property name="maxLength" value="19" />
        <property name="validationPattern">
            <bean parent="NumericValidationPattern" />
        </property>
        <property name="control">
            <bean parent="TextControlDefinition" p:size="30" />
        </property>
        <property name="name" value="sortKey" />
        <property name="label" value="Sort Key" />
    </bean>

    <bean id="EnumerationValue-lookupDefinition" parent="EnumerationValue-lookupDefinition-parentBean" />

    <bean id="EnumerationValue-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
        <property name="lookupFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="enumerationTableId" />
                <bean parent="FieldDefinition" p:attributeName="code" />
            </list>
        </property>
        <property name="resultFields">
            <list>
                <bean parent="FieldDefinition" p:attributeName="id" />
                <bean parent="FieldDefinition" p:attributeName="enumerationTableId" />
                <bean parent="FieldDefinition" p:attributeName="code" />
                <bean parent="FieldDefinition" p:attributeName="abbrevValue" />
                <bean parent="FieldDefinition" p:attributeName="value" />
            </list>
        </property>
        <property name="title" value="Enumeration Value Lookup" />
    </bean>

    <bean id="EnumerationValue-inquiryDefinition" parent="EnumerationValue-inquiryDefinition-parentBean" />

    <bean id="EnumerationValue-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
        <property name="inquirySections">
            <list>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Enumeration Value" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="FieldDefinition" p:attributeName="id" />
                            <bean parent="FieldDefinition" p:attributeName="enumerationTableId" />
                            <bean parent="FieldDefinition" p:attributeName="code" />
                            <bean parent="FieldDefinition" p:attributeName="abbrevValue" />
                            <bean parent="FieldDefinition" p:attributeName="value" />
                            <bean parent="FieldDefinition" p:attributeName="active" />
                            <bean parent="FieldDefinition" p:attributeName="effectiveDate" />
                            <bean parent="FieldDefinition" p:attributeName="expirationDate" />
                            <bean parent="FieldDefinition" p:attributeName="sortKey" />
                        </list>
                    </property>
                </bean>
                <bean parent="InquirySectionDefinition">
                    <property name="title" value="Enumeration Contexts" />
                    <property name="numberOfColumns" value="1" />
                    <property name="inquiryFields">
                        <list>
                            <bean parent="InquiryCollectionDefinition">
                                <property name="attributeName" value="enumerationContextValueList" />
                                <property name="businessObjectClass"
                                    value="org.kuali.student.core.enumerationmanagement.bo.EnumerationContextValue" />
                                <property name="numberOfColumns" value="1" />
                                <property name="summaryTitle" value="Enumeration Context" />
                                <property name="inquiryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="contextId" />
                                        <bean parent="FieldDefinition" p:attributeName="enumerationContext.contextKey" />
                                        <bean parent="FieldDefinition" p:attributeName="active" />
                                    </list>
                                </property>
                                <property name="summaryFields">
                                    <list>
                                        <bean parent="FieldDefinition" p:attributeName="enumerationContext.contextKey" />
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>

</beans>
    