<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-2.0.xsd
                           http://www.springframework.org/schema/tx
                           http://www.springframework.org/schema/tx/spring-tx-2.0.xsd
                           http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-2.0.xsd">
          
    <bean id="kstestModuleConfiguration" class="org.kuali.rice.kns.bo.ModuleConfiguration">
        <property name="namespaceCode" value="kstest"/>
        <property name="databaseRepositoryFilePaths">
            <list>
                <value>OJB-repository-kstest.xml</value>
            </list>
        </property>
        <property name="packagePrefixes">
            <list>
                <value>org.kuali.student.core.messages</value>
                <value>org.kuali.student.core.enumerationmanagement</value>
            </list>
        </property>
        <property name="initializeDataDictionary" value="true"/>
        <property name="dataDictionaryPackages">
            <list>
                <value>classpath:org/kuali/student/core/enumerationmanagement/bo/datadictionary/EnumerationTable.xml</value>
                <value>classpath:org/kuali/student/core/enumerationmanagement/bo/datadictionary/EnumerationTableContextView.xml</value>
                <value>classpath:org/kuali/student/core/enumerationmanagement/bo/datadictionary/EnumerationContext.xml</value>
                <value>classpath:org/kuali/student/core/enumerationmanagement/bo/datadictionary/EnumerationContextValue.xml</value>
                <value>classpath:org/kuali/student/core/enumerationmanagement/bo/datadictionary/EnumerationValue.xml</value>
                <value>classpath:org/kuali/student/core/enumerationmanagement/document/datadictionary/EnumerationValueMaintenanceDocument.xml</value>
                <value>classpath:org/kuali/student/core/enumerationmanagement/document/datadictionary/EnumerationTableMaintenanceDocument.xml</value>
                <value>classpath:org/kuali/student/core/enumerationmanagement/document/datadictionary/EnumerationTableContextViewMaintenanceDocument.xml</value>
                <value>classpath:org/kuali/student/core/messages/bo/datadictionary/MessageEntity.xml</value>
                <value>classpath:org/kuali/student/core/messages/document/datadictionary/MessageEntityMaintenanceDocument.xml</value>
            </list>
        </property>
    </bean>

    <bean id="kstestModuleService" class="org.kuali.rice.kns.service.impl.ModuleServiceBase">
        <property name="moduleConfiguration" ref="kstestModuleConfiguration" />
    </bean>
    
    <bean class="org.kuali.rice.core.resourceloader.RiceSpringResourceLoaderConfigurer">
        <property name="localServiceName" value="KSCoreContext"/>
    </bean>
    
    
    <bean id="ksMessageServiceImpl" class="org.kuali.student.core.messages.service.impl.MessageServiceImpl" />
    
    <bean class="org.kuali.rice.ksb.messaging.KSBExporter">
        <property name="serviceDefinition">
            <bean class="org.kuali.rice.ksb.messaging.SOAPServiceDefinition">
                <property name="jaxWsService" value="true" />
                <property name="service" ref="ksMessageServiceImpl" />
                <property name="serviceInterface" value="org.kuali.student.core.messages.service.MessageService" />
                <property name="localServiceName" value="MessageService" />
                <property name="serviceNameSpaceURI" value="http://student.kuali.org/wsdl/messages" />
                <property name="busSecurity" value="false" />
            </bean>
        </property>
    </bean>
    
</beans>